{% extends 'base.html.twig' %}

{% block title %} Mes sessions {% endblock %}

{% block body %}
    <section class="utilisateurHeader">
        <div class="consumableHeader">
            <header>
                <a href="/" class="logo">StonkSurfer</a>
                <ul id="navbar">
                    {% if app.user %}
                        <li>
                            <a href="/logout"><i class="fas fa-user-slash"></i> Déconnexion</a>
                        </li>
                        <li>
                            <a href="/utilisateur"><i class="fas fa-user"></i> Mon compte</a>
                        </li>
                    {% else %}
                        <li>
                            <a href="/register"><i class="fas fa-user-plus"></i> S'inscrire</a>
                        </li>
                        <li>
                            <a href="/login"><i class="fas fa-user"></i> Se connecter</a>
                        </li>
                    {% endif %}

                </ul>

            </header>

        </div>
    </section>
    <div class="utilisateurContainer">
        <a href="/utilisateur">Retour</a>
        <div class="utilisateurSessions">

            <div class="utilisateurSessionsContent">
                <div class="utilisateurSessionsContentBx">
                    <a href="/utilisateur/session">Ajouter une session</a>
                </div>
                <div class="utilisateurSessionsContentBx">
                    <a href="/utilisateur">Fil d'actualité</a>
                </div>
                <div class="utilisateurSessionsContentBx">
                    <a href="/utilisateur/consumable">Liste consommables</a>
                </div>

                <div class="utilisateurSessionsContentBx">
                    <a href="/utilisateur/ajouterConsumable">Ajouter consommable</a>
                </div>

                <div class="utilisateurSessionsContentBx">
                    <a href="/spotcreation">Ajouter un spot</a>
                </div>
            </div>
        </div>
        <div class="utilisateurFil">
            <h2 class="title">Mes sessions </h2>
            {% for session in sessions %}
                <div class="cardUtilisateurContenu">


                    <p>Post créé par : {{ session.idUtilisateur.email }}</p>
                    <p>Le : {{ session.dateSessionDebut|date('d/m/Y H:i') }}</p>
                    <p>{{ session.idSpot.Lieu }}</p>
                    <p>{{ session.idSpot.Ville }}</p>
                    <p>{{ session.commentaire }}</p>
                    <p>Note performance de la session :
                        {% for i in 1.. session.notePerformance  %}
                            <img src="data:image/svg+xml;base64," />
                        {% endfor %}
                        {% set reste = 5 - session.notePerformance %}
                        {% if reste>0 %}
                            {% for a in 0..reste  %}
                                <img src="data:image/svg+xml;base64," />
                            {% endfor  %}
                        {% endif %}
                    </p>
                    <p>Note écologique du spot :
                        {% for i in 1.. session.noteEcologique  %}
                            <img  src="data:image/svg+xml;base64," />
                        {% endfor %}
                        {% set reste = 5 - session.noteEcologique %}
                        {% if reste>0 %}
                            {% for a in 0..reste-1  %}
                                <img  src="data:image/svg+xml;base64," />
                            {% endfor  %}
                        {% endif %}
                    </p>
                    <p>Note des infrastructures du spot :
                        {% for i in 1.. session.noteInfrastructure  %}
                            <img  src="data:image/svg+xml;base64," />
                        {% endfor %}
                        {% set reste = 5 - session.noteInfrastructure %}
                        {% if reste>0 %}
                            {% for a in 0..reste-1  %}
                                <img  src="data:image/svg+xml;base64," />
                            {% endfor  %}
                        {% endif %}
                    </p>

                </div>
                <center>
                    <div class="utilisateurSessionsContentBx mt-3 mb-3">
                        <a href="/utilisateur/sessions/{{ session.id }}">Modifier</a>
                    </div>
                </center>

            {% endfor %}
        </div>
        <svg>
            <filter id="wavy">
                <feTurbulence x="0" y="0" baseFrequency="0.02" numOctaves="5" seed="2">
                    <animate attributeName="baseFrequency" dur="60s" values="0.02;0.07;0.03;" repeatCount="indefinite" />
                </feTurbulence>
                <feDisplacementMap in="SourceGraphic" scale="30"/>
            </filter>
        </svg>
    </div>
{% endblock %}
